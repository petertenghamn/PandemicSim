<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="393.0" prefWidth="882.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Screen.Controller.StatsController">
   <children>
      <LineChart fx:id="lineChart" layoutX="14.0" layoutY="20.0" prefHeight="323.0" prefWidth="457.0" title="Average Population Growth">
        <xAxis>
          <CategoryAxis label="Time in years" side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" label="Population" side="LEFT" />
        </yAxis>
      </LineChart>
      <PieChart fx:id="pieChart" layoutX="472.0" layoutY="20.0" prefHeight="323.0" prefWidth="396.0" title="Average Population Size" />
      <Button fx:id="backBtn" layoutX="14.0" layoutY="354.0" mnemonicParsing="false" onAction="#backBtnAction" text="Back" />
   </children>
</AnchorPane>
